{
  "name": "datepicker-for-grid",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "datepicker for grid",
  "content_scripts": [
    {
      "matches": [
        "https://s.questetra.net/*"
      ],
      "js": [
        "jquery-3.3.1.min.js",
        "jquery-ui.min.js",
        "content-script.js"
      ],
      "css": [
        "jquery-ui.min.css",
        "content-script.css"
      ]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "16": "resource/icon_x16.png",
      "48": "resource/icon_x48.png",
      "128": "resource/icon_x128.png"
    },
    "default_title": "datepicker-for-grid",
    "default_popup": "popup.html"
  },
  "permissions": [
    "clipboardRead"
  ],
  "web_accessible_resources": [
    "resource/icon_x128.png"
  ],
  "icons": {
    "16": "resource/icon_x16.png",
    "48": "resource/icon_x48.png",
    "128": "resource/icon_x128.png"
  }
}
